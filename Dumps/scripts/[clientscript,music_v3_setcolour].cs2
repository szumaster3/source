// 832
[clientscript,music_v3_setcolour]
def_int $int0 = 0;
def_int $int1 = 0;
def_int $int2 = 0;
while ($int0 <= 646) {
	if (cc_find(music_v3:com_1, $int0) = ^true & string_length(cc_gettext) > 0) {
		if (~music_getvar($int0) = 1) {
			cc_setcolour(^green);
			$int1 = calc($int1 + 1);
		} else {
			cc_setcolour(^red);
		}
		$int2 = calc($int2 + 1);
	}
	$int0 = calc($int0 + 1);
}
if_settext("Unlocked:<br><tostring($int1)> / <tostring($int2)>", music_v3:unlock_counter);
